{
  "name": "korzh-website",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "type": "module",
  "scripts": {
    "clear:metro": "shx rm -rf source/assets/metro/*",
    "start": "cross-env RUBYOPT=-W0 bundle exec jekyll serve --livereload --incremental",
    "watch": "webpack watch --mode=development --config ./scripts/webpack.config.js",
    "build": "webpack --mode=production --config ./scripts/webpack.config.js",
    "build:metro": "npm run clear:metro && cross-env MODE=production node ./scripts/build-metro.js",
    "compile:css": "lessc --source-map -clean-css source/assets/css/index.less source/assets/css/index.css"
  },
  "keywords": [],
  "author": "Korzh.com",
  "license": "MIT",
  "devDependencies": {
    "@netlify/plugin-sitemap": "^0.8.1",
    "@olton/esbuild-plugin-autoprefixer": "^0.2.2",
    "@olton/esbuild-plugin-progress": "^0.4.0",
    "autoprefixer": "^10.4.22",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^10.1.0",
    "css-loader": "^7.1.2",
    "esbuild": "^0.26.0",
    "esbuild-plugin-less": "^1.3.31",
    "esbuild-plugin-replace": "^1.4.0",
    "less": "^4.4.2",
    "less-loader": "^12.3.0",
    "less-plugin-clean-css": "^1.6.0",
    "shx": "^0.4.0",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "dependencies": {
